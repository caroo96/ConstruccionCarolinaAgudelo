package app.domain.models;

import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Setter	
@Getter 
@NoArgsConstructor
public class Pet {
	
	private long petId;
	private String name;
	private long documentOwner;
	private int age;
	private Species species;
	private Race race;
	private String characteristics;
	private int weight;
	
	
	public Pet(long petId, String name, long documentOwner, int age, Species species, Race race, String characteristics,
			int weight) {
		super();
		this.petId = petId;
		this.name = name;
		this.documentOwner = documentOwner;
		this.age = age;
		this.species = species;
		this.race = race;
		this.characteristics = characteristics;
		this.weight = weight;
	}
	
	

}
